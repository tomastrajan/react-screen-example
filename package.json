{
  "name": "react-screen-example",
  "version": "1.0.0",
  "description": "Implementation of a example screen (table + modal form) with React JS to realistically evaluate amount of effort needed",
  "main": "src/main.tsx",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --TARGET=DEV",
    "build": "webpack && node server.js",
    "dist": "webpack --TARGET=DIST && node server.js --TARGET=DIST",
    "ci": "npm run test && webpack",
    "lint": "tslint src/**/*.ts",
    "mocha": "tsc -p . && mocha .tmp/**/*-test.js",
    "test": "npm run lint && npm run mocha",
    "watch": "watch '(tslint src/**/*.ts && tsc -p . && mocha .tmp/**/*-test.js) || true' src/"
  },
  "dependencies": {
    "events": "1.1.0",
    "lodash": "3.10.1",
    "react": "0.14.3",
    "react-dom": "0.14.3"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "chalk": "1.1.1",
    "clean-webpack-plugin": "0.1.3",
    "connect": "3.4.0",
    "css-loader": "0.20.1",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "1.6.2",
    "minimist": "1.2.0",
    "mocha": "2.3.4",
    "open-browser-webpack-plugin": "0.0.1",
    "react-hot-loader": "1.3.0",
    "sinon": "1.17.2",
    "style-loader": "0.12.4",
    "svg-loader": "0.0.2",
    "ts-loader": "0.7.2",
    "tslint": "3.2.1",
    "typescript": "1.7",
    "typescript-require": "0.2.9",
    "url-loader": "0.5.6",
    "watch": "^0.17.1",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomastrajan/react-screen-example.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "example",
    "table",
    "modal",
    "form"
  ],
  "author": "Tomas Trajan <tomas.trajan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomastrajan/react-screen-example/issues"
  },
  "homepage": "https://github.com/tomastrajan/react-screen-example#readme"
}
